	function detectPopupBlocker()
	{ 
		var test = window.open("about:blank","","directories=no,height=100,width=100,menubar=no,resizable=no,scrollbars=no,status=no,titlebar=no,top=5000,location=no");
		if(test)
		{ // not blocked 
			test.close(); 
			return 1; 
		} 
		else
		{ // blocked
			return 0; 
		} 
	}

    function CustomFinishLoad()
	{
		if (BrowserDetect.browser == "Explorer" || BrowserDetect.browser == "Firefox" || BrowserDetect.browser == "Safari")
		{
			if (BrowserDetect.browser == "Explorer")
			{
				if (detectPopupBlocker() == 0) document.getElementById('popUpWarning').style.display = "";
			}
		}
		else window.location = "http://www.a.com/remote/update.asp?app=owa&sasrc=" + encodeURI(window.location.href);
		FinishLoad(1);
		hideJSWarn();
	}
	
